        <xs:complexType>
            <xs:sequence>
                <xs:element name="COMPANYID" type="xs:int"/>
                <xs:element name="FINANCIAL_STATEMENT_DATE" type="xs:string"/>
                <xs:element name="COUNTRYISOCODE" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="getStudentResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="TOTAL_ASSETS_IMPUTED" type="xs:int"/>
