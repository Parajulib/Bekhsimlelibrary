	            <xs:element name="TOTAL_ASSETS_PERCONTR" type="xs:int"/>
	            <xs:element name="LOAN_OVER_DEPOSITS_IMPUTED" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
</xs:schema> 